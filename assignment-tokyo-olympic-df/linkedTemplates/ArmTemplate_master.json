{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "assignment-tokyo-olympic-df"
		},
		"AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage'"
		},
		"Athletes_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Athletes.csv"
		},
		"AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://assigmenttokyoolympic.dfs.core.windows.net/"
		},
		"Coaches_source_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Coaches.csv"
		},
		"EntriesGender_source_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/EntriesGender.csv"
		},
		"Medals_source_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Medals.csv"
		},
		"Teams_source_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/darshilparmar/tokyo-olympic-azure-data-engineering-project/main/data/Teams.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "assignment-tokyo-olympic-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_accountKey')]"
					},
					"Athletes_source_properties_typeProperties_url": {
						"value": "[parameters('Athletes_source_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"Coaches_source_ls_properties_typeProperties_url": {
						"value": "[parameters('Coaches_source_ls_properties_typeProperties_url')]"
					},
					"EntriesGender_source_ls_properties_typeProperties_url": {
						"value": "[parameters('EntriesGender_source_ls_properties_typeProperties_url')]"
					},
					"Medals_source_ls_properties_typeProperties_url": {
						"value": "[parameters('Medals_source_ls_properties_typeProperties_url')]"
					},
					"Teams_source_ls_properties_typeProperties_url": {
						"value": "[parameters('Teams_source_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}